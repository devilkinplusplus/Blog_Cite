<p>Hər gün yeni məlumatlar</p>
<form method="post">
    <input type="email" id="txtmail" placeholder="Mail" name="NewsMail" required="">
    <input type="submit" id="btn" value="İzlə">
</form>


<script src="https://code.jquery.com/jquery-3.6.0.min.js"
        type="text/javascript">
</script>

<script type="text/javascript">
    $("#btn").click(function(){
        let values ={
            NewsMail:$("#txtmail").val()
        };
        $.ajax({
            type:"post",
            url:"/Blog/Subs/Subscribe/",
            data:values,
            succes:function(func){
                let result = jQuery.parseJSON(func);
            }
        })
    })

</script>

